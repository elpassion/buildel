{
  "name": "@buildel/buildel",
  "private": false,
  "version": "0.3.2",
  "license": "MIT",
  "description": "A flexible library for integrating with the Buildel service via WebSockets. Enables easy connection, authentication, and real-time event handling within the context of organizations and pipelines.",
  "keywords": [
    "javascript",
    "buildel",
    "websocket",
    "integration",
    "real-time",
    "event-handling",
    "pipelines",
    "organizations",
    "typescript",
    "connection"
  ],
  "type": "module",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "npm:publish": "npm publish --access public",
    "npm:publish:preview": "npm publish --dry-run",
    "dev": "npm run build -- --watch",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/phoenix": "^1.6.0",
    "@types/uuid": "^9.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "phoenix": "^1.7.7",
    "uuid": "^9.0.1"
  },
  "files": [
    "dist"
  ]
}
